{
  "name": "rr7-cf-workers",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "start": "wrangler dev",
    "typecheck": "react-router typegen && tsc",
    "preview": "pnpm build && wrangler dev",
    "typegen:cf": "wrangler types",
    "typegen": "react-router typegen",
    "deploy:preview": "pnpm build && wrangler deploy --env preview",
    "deploy:production": "pnpm build && wrangler deploy",
    "db:generate": "dotenv -- drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply --local DB",
    "db:migrate:preview": "wrangler d1 migrations apply DB --remote --env preview",
    "db:migrate:production": "wrangler d1 migrations apply DB --remote",
    "db:seed": "pnpm wrangler d1 execute --local DB --file ./drizzle/seed/seed.sql",
    "db:seed:preview": "pnpm wrangler d1 execute --remote --preview DB --file ./drizzle/seed/seed.sql",
    "db:seed:production": "pnpm wrangler d1 execute --remote DB --file ./drizzle/seed/seed.sql",
    "db:studio": "drizzle-kit studio --config ./drizzle/local-studio.config.ts",
    "db:studio-production": "drizzle-kit studio"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250504.0",
    "@react-router/cloudflare": "^7.5.3",
    "@react-router/fs-routes": "^7.5.3",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.5",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "tailwindcss": "^4.1.5",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.14.1"
  },
  "packageManager": "pnpm@10.9.0+sha256.ea53ab747ac8b7921de63b42eab1adf021e1fe6ded05f62f00885cc33a46118d"
}
